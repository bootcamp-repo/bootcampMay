package com.bootcamp.lunch.model;


/**
 * @author Sweekar
 *
 */
public class Chipotle {
	
	private int id;
	private String name;
	private String meat;
	private String taco_quantity;
	private String tortilla;
	private String rice;
	private String beans;
	private String toppings;
	
	
	/**
	 * @return the id
	 */
	public int getId() {
		return id;
	}
	/**
	 * @param id the id to set
	 */
	public void setId(int id) {
		this.id = id;
	}
	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}
	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}
	/**
	 * @return the meat
	 */
	public String getMeat() {
		return meat;
	}
	/**
	 * @param meat the meat to set
	 */
	public void setMeat(String meat) {
		this.meat = meat;
	}
	/**
	 * @return the taco_quantity
	 */
	public String getTaco_quantity() {
		return taco_quantity;
	}
	/**
	 * @param taco_quantity the taco_quantity to set
	 */
	public void setTaco_quantity(String taco_quantity) {
		this.taco_quantity = taco_quantity;
	}
	/**
	 * @return the tortilla
	 */
	public String getTortilla() {
		return tortilla;
	}
	/**
	 * @param tortilla the tortilla to set
	 */
	public void setTortilla(String tortilla) {
		this.tortilla = tortilla;
	}
	/**
	 * @return the rice
	 */
	public String getRice() {
		return rice;
	}
	/**
	 * @param rice the rice to set
	 */
	public void setRice(String rice) {
		this.rice = rice;
	}
	/**
	 * @return the beans
	 */
	public String getBeans() {
		return beans;
	}
	/**
	 * @param beans the beans to set
	 */
	public void setBeans(String beans) {
		this.beans = beans;
	}
	/**
	 * @return the toppings
	 */
	public String getToppings() {
		return toppings;
	}
	/**
	 * @param toppings the toppings to set
	 */
	public void setToppings(String toppings) {
		this.toppings = toppings;
	}
	
	
	
	
	
}
